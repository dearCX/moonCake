/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.index-container{
  width: 100%;
  height: 100%;
  position: relative;
}
.btns{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 60;
  width: 100%;
  padding-top: 95%;
  text-align: center;
}
.btns button{
  width: 194px;
  height: 38px;
  border-radius: 18px;
  background-color: transparent;
  line-height: 38px;
}
.submit-btn{
  display: inline-block;
  margin-bottom: 15px;
  color: #2D3153;
  background: url('data:image/png;base64,') no-repeat center top;
}
.my-btn{
  border:1px solid #FCCB1C;
  color: #FCCB1C;
  font-family: 'FZZZHONGJW--GB1-0' 16px;
}
.main-container{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background-color: #2D3152;
}
.bg{
  width: 100%;
  height: 100%;
}
.bottom-bg{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 30;
  width: 100%;
}